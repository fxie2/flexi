#!MC 1400
# Created by Tecplot 360 build 14.0.2.35002
$!VarSet |PATH|       = 'FIGIPATH'
# Polydeg
$!VarSet |MYN1|       = 'FIGIMYN1'
$!VarSet |MYN2|       = 'FIGIMYN2'
$!VarSet |MYN3|       = 'FIGIMYN3'
# Path of datasets
$!VarSet |INFILE1|    = 'FIGIINFILE1'
$!VarSet |INFILE2|    = 'FIGIINFILE2'
$!VarSet |INFILE3|    = 'FIGIINFILE3'

$!READDATASET  '"|INFILE1|"'
  READDATAOPTION = NEW
  RESETSTYLE = YES
  INCLUDETEXT = NO
  INCLUDEGEOM = NO
  INCLUDECUSTOMLABELS = NO
  VARLOADMODE = BYNAME
  ASSIGNSTRANDIDS = YES
  INITIALPLOTTYPE = XYLINE
  VARNAMELIST = '"CoordinateX" "WallFrictionX"'
$!READDATASET  '"|INFILE2|"'
  READDATAOPTION = APPEND
  RESETSTYLE = NO
  INCLUDETEXT = NO
  INCLUDEGEOM = NO
  INCLUDECUSTOMLABELS = NO
  VARLOADMODE = BYNAME
  ASSIGNSTRANDIDS = YES
  INITIALPLOTTYPE = XYLINE
  VARNAMELIST = '"CoordinateX" "WallFrictionX"'
$!READDATASET  '"|INFILE3|"'
  READDATAOPTION = APPEND
  RESETSTYLE = NO
  INCLUDETEXT = NO
  INCLUDEGEOM = NO
  INCLUDECUSTOMLABELS = NO
  VARLOADMODE = BYNAME
  ASSIGNSTRANDIDS = YES
  INITIALPLOTTYPE = XYLINE
  VARNAMELIST = '"CoordinateX" "WallFrictionX"'
$!VIEW FIT
$!XYLINEAXIS YDETAIL 1 {TITLE{TITLEMODE = USETEXT}}
$!XYLINEAXIS YDETAIL 1 {TITLE{TEXT = 'WallFriction'}}
$!XYLINEAXIS YDETAIL 1 {TITLE{OFFSET = 7}}
$!XYLINEAXIS XDETAIL 1 {TITLE{TITLEMODE = USETEXT}}
$!XYLINEAXIS XDETAIL 1 {TITLE{TEXT = 'x/h'}}
$!XYLINEAXIS GRIDAREA{DRAWBORDER = YES}
$!GLOBALLINEPLOT 
  LEGEND
    {
    SHOW = YES
    TEXTSHAPE
      {
      HEIGHT = 2.5
      }
    BOX
      {
      BOXTYPE = NONE
      }
    XYPOS
      {
      X = 40
      Y = 84
      }
    }

$!CREATELINEMAP 
$!DUPLICATELINEMAP 
  SOURCEMAP = 1
  DESTINATIONMAP = 2
$!CREATELINEMAP 
$!DUPLICATELINEMAP 
  SOURCEMAP = 1
  DESTINATIONMAP = 3
$!ACTIVELINEMAPS += [1]
$!ACTIVELINEMAPS += [2]
$!ACTIVELINEMAPS += [3]

$!LINEMAP [1]
  NAME = 'DG N=|MYN1|'
  ASSIGN{ZONE = 1}
  LINES
    {
    COLOR = BLACK
    LINEPATTERN = DASHDOT
    PATTERNLENGTH = 1.5
    LINETHICKNESS = 0.2
    }
$!LINEMAP [2]
  NAME = 'DG N=|MYN2|'
  ASSIGN{ZONE = 2}
  LINES
    {
    SHOW = YES
    COLOR = BLACK
    LINEPATTERN = DASHED
    PATTERNLENGTH = 1.0
    LINETHICKNESS = 0.2
    }
$!LINEMAP [3]
  NAME = 'DG N=|MYN3|'
  ASSIGN{ZONE = 3}
  LINES
    {
    SHOW = YES
    COLOR = BLACK
    LINEPATTERN = SOLID
    PATTERNLENGTH = 1.0
    LINETHICKNESS = 0.2
    }
$!ATTACHTEXT 
  ANCHORPOS
    {
    X = 42.48782089089214
    Y = 36.50568181818181
    }
  TEXTSHAPE
    {
    HEIGHT = 18
    }
  TEXT = 'upper wall'
$!ATTACHTEXT 
  ANCHORPOS
    {
    X = 36.93259600348464
    Y = 22.44318181818181
    }
  TEXTSHAPE
    {
    HEIGHT = 18
    }
  TEXT = 'lower wall'
$!XYLINEAXIS YDETAIL 1 {RANGEMIN = -0.0050000000000000000}
$!XYLINEAXIS YDETAIL 1 {RANGEMAX = 0.0250000000000000000}
$!REDRAW 
$!EXPORTSETUP EXPORTFORMAT = EPS
$!EXPORTSETUP IMAGEWIDTH = 1275
$!EXPORTSETUP EPSPREVIEWIMAGE{IMAGETYPE = NONE}
$!EXPORTSETUP EXPORTFNAME = '|PATH|/phill_wallfriction.eps'
$!EXPORT 
  EXPORTREGION = CURRENTFRAME
$!EXPORTSETUP EXPORTFORMAT = PNG
$!EXPORTSETUP IMAGEWIDTH = 600
$!EXPORTSETUP USESUPERSAMPLEANTIALIASING = YES
$!EXPORTSETUP EXPORTFNAME = '|PATH|/phill_wallfriction.png'
$!EXPORT 
  EXPORTREGION = CURRENTFRAME
$!SAVELAYOUT  "|PATH|/phill_wallfriction.lpk"
  INCLUDEDATA = YES
  INCLUDEPREVIEW = NO
